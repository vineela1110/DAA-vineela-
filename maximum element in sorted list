#include <stdio.h>
void sort(int arr[], int n) {
}
int findMax(int arr[], int n) {
    return arr[n - 1]; 
}
int main() {
    int arr[] = {3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    sort(arr, n);
    int max = findMax(arr, n);
    printf("Maximum element in the list: %d\n", max);
    return 0;
}
OUTPUT:
Maximum element in the list: 5
