#include<stdio.h>
int main() {
    int num1, num2, i, gcd;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    for (i = 1; i <= num1 && i <= num2; ++i) {
        if (num1 % i == 0 && num2 % i == 0) {
            gcd = i;
        }
    }
    printf("GCD of %d and %d is %d", num1, num2, gcd);
    return 0;
}
Output:
Enter two numbers: 4 5
GCD of 4 and 5 is 1

